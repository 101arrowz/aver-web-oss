{
  "name": "aver-web",
  "version": "0.0.1",
  "description": "Audiovisual emotion recognition for the web",
  "repository": "https://github.com/101arrowz/aver-web",
  "author": "Arjun Barrett",
  "license": "MIT",
  "scripts": {
    "start": "parcel src/index.html",
    "create-tfjs": "tfjs-custom-module --config .tfjsrc",
    "build": "parcel build --no-source-maps src/index.html",
    "lint": "eslint --fix \"src/**/*.{ts,tsx}\" && tsc",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@babel/preset-env": "^7",
    "@babel/preset-react": "^7",
    "@babel/preset-typescript": "^7",
    "@emotion/babel-plugin": "^11",
    "@emotion/core": "^10",
    "@parcel/packager-raw-url": "2.0.0-beta.2",
    "@parcel/packager-ts": "2.0.0-beta.2",
    "@parcel/transformer-image": "2.0.0-beta.2",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^7",
    "eslint-config-prettier": "^7",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^4",
    "http-server": "^0.12.3",
    "parcel": "2.0.0-beta.2",
    "parcel-config-precache-manifest": "^0.0.3",
    "parcel-config-pwa-manifest": "^0.0.9",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2",
    "react": "^17",
    "react-dom": "^17",
    "typescript": "^4"
  },
  "dependencies": {
    "@emotion/react": "^11",
    "@tensorflow/tfjs": "^3.3.0",
    "@tensorflow/tfjs-backend-wasm": "^3.4.0",
    "mdi-react": "^7.5.0",
    "preact": "^10",
    "react-universal-state": "^1.3.0",
    "rmwc": "^6"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "@tensorflow/tfjs": "./tfjs/custom_tfjs.js",
    "@tensorflow/tfjs-core/dist/ops/ops_for_converter": "./tfjs/custom_ops_for_converter.js"
  }
}
